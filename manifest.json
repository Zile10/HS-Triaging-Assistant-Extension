{
    "name": "Triage Assistant",
    "version": "0.0.1",
    "manifest_version": 3,
    "description": "Automate the triaging process...",
    "permissions": ["webNavigation", "activeTab"],
    "background": {
        "service_worker": "src/js/background.js"
    },
    "content_scripts": [
        {
            "matches": ["https://secure.helpscout.net/*"],
            "js": ["src/js/content/note.js", "src/js/content/customFields.js", "src/js/content/aidMenu.js", "src/js/content/init.js"],
            "css": ["src/css/aidMenu.css"]
        }
    ]
}